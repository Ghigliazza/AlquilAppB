<div id="<%= dom_id report %>">
  <p>
    <strong>Description:</strong>
    <%= report.description %>
  </p>

  <p>
    <strong>User:</strong>
    <%= report.user_id %>
  </p>

  <p>
    <strong>Car:</strong>
    <%= report.car_id %>
  </p>

  <p>
  <strong>Image</strong>
  <%= image_tag @report.image, :alt => 'report_image' %>
  </p>

</div>
